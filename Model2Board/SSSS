 INSERT INTO BOARD (SEQ, WRITER, EMAIL, SUBJECT, PASSWORD, REG_DATE, REF, RE_STEP, RE_LEVEL, READCOUNT, CONTENT) 
VALUES (SEQ_BOARD.NEXTVAL, 'ㅎㅇ', 'GG@GG', 'HEHE', 153, SYSDATE, 7, 8, 9, 0, 'GDGD' );

SELECT * FROM BOARD;

CREATE TABLE Board
( 	
	SEQ NUMBER(8) PRIMARY KEY,
	NUM NUMBER(3) ,
	WRITER VARCHAR2(100) NOT NULL,
	EMAIL VARCHAR2(100) NOT NULL,
	SUBJECT VARCHAR2(100) NOT NULL,
	PASSWORD VARCHAR2(100) NOT NULL,
	REG_DATE DATE,
	REF NUMBER(3) NOT NULL,
	RE_STEP NUMBER(3) NOT NULL,
	RE_LEVEL NUMBER(3) NOT NULL,
	READCOUNT NUMBER(3) NOT NULL,
	CONTENT VARCHAR2(100) NOT NULL
	) ;
	
	DROP TABLE BOARD;
	
INSERT INTO BOARD
values (SEQ_BOARD.NEXTVAL, 13,'13번째', 'ㄱㄱ', 'ㄴㄴ', 'ㄹㄹ', 'ㅁㅁ', 2, 2, 10, 15,'췍');

시퀀스 추가
CREATE SEQUENCE SEQ_BOARD
START WITH 1
INCREMENT BY 1;

